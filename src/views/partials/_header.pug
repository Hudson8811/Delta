header.header
	.container.header__container 
		img(src="./img/frame.svg", alt="").frame.header__frame
		.header__inner
			if page.home === true
				a(href="https://www.delta.ru/" target="_blank").header__logo
					img(src="./img/logo.svg", alt="")
			else
				a(href="/index.html").header__logo
					img(src="./img/logo.svg", alt="")

			nav.header__nav 
				ul 
					li 
						a(href="/game-house.html") дом и домочадцы
					li 
						a(href="/game-car.html") автомобиль
					li 
						a(href="/game-values.html") ценности
					li 
						a(href="/promo.html") промо
			button.header__button
				<svg class="header__button-icon">
					<use xlink:href="img/sprite.svg#graph"></use>
				</svg>
			a(target="_blank" href="https://woman.ru").header__link
				img(src="img/woman.svg", alt="")
		img(src="img/frame.svg", alt="").frame.header__frame.header__frame--right